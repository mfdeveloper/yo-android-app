    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Yeoman android-app",
            "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
            "args": [
                "android-app"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Yeoman android-app:lib",
            "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
            "args": [
                "android-app:lib"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}